{
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": { "arrowFunctions": true, "modules": true, "blockBindings": true },
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off"
    }
}
